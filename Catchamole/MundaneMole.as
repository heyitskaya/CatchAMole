package  {		import flash.display.MovieClip; //import stuff	import flash.events.MouseEvent;			public class MundaneMole extends MovieClip 	{		private var mundaneMoleScore:int=1;		//private var catchMeBox.text.visible=false;\		//catchMeBox.text.visible=false;								public function MundaneMole() 		{			catchMeBox.visible=false;						this.addEventListener(MouseEvent.CLICK, clearMundaneMole);			this.addEventListener(MouseEvent.MOUSE_OVER, displayComments) //add and event listener to Mundane Mole that calls displayComments upon click			// constructor code		}				public function clearMundaneMole(e:MouseEvent):void		{			parent.removeChild(this); //this removes the mole		}				public function displayComments(e:MouseEvent):void //triggered by mouseevent 		{			catchMeBox.visible=true; //make the catchMeBox visible		}	}		}