package  {		import flash.display.MovieClip; //import stuff	import flash.events.MouseEvent;			public class RabidMole extends MovieClip 	{				private var numClicks:int=0 //set numClicks to 0						public function RabidMole() 		{			rabiesBox.visible=false; //set the visibility of the rabiesBox to false			this.addEventListener(MouseEvent.CLICK,clearRabidMole);// constructor code			this.addEventListener(MouseEvent.MOUSE_OVER,displayRabies); //mouseevent to trigger function dispaly rabies upon click		}				public function clearRabidMole(e:MouseEvent):void //parameter MouseEvent		{			numClicks++; //numClciks increments everytime you click it									if (numClicks>=3) //if numClicks is greater than 3			{				parent.removeChild(this); //the mole gets removed			}		}				public function displayRabies(e:MouseEvent):void //when mole gets clicked		{			rabiesBox.visible=true; //the rabiesBox will be set to visible		}							}	}